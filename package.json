{
  "name": "munchkin-material",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config=webpack/config.development.js",
    "dist": "cross-env NODE_ENV=production webpack --config=webpack/config.production.js",
    "doc": "esdoc",
    "i18n:download": "node scripts/i18n/download.js",
    "i18n:translate": "node scripts/i18n/translate.js",
    "i18n:upload": "node scripts/i18n/upload.js",
    "install": "npm run-script dist",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:js": "eslint --ext .js,.jsx .",
    "site": "cross-env NODE_ENV=production webpack --config=webpack/config.site.js",
    "start": "cross-env NODE_ENV=production webpack-dev-server --config=webpack/config.site.js",
    "stats": "cross-env NODE_ENV=production webpack --config=webpack/config.stats.js",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "engines": {
    "node": ">=6"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "./src/constants/",
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "notify": true,
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js"
  },
  "dependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-1": "6.24.1",
    "classnames": "2.2.5",
    "cname-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.28.0",
    "lodash": "4.17.4",
    "material-ui": "0.18.3",
    "munchkin": "github:izorg/munchkin",
    "normalize.css": "7.0.0",
    "offline-plugin": "4.8.1",
    "postcss-cssnext": "2.11.0",
    "postcss-loader": "1.3.3",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-helmet": "5.1.3",
    "react-hot-loader": "3.0.0-beta.7",
    "react-intl": "2.3.0",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-slick": "0.14.11",
    "react-sortable-hoc": "0.6.3",
    "react-tap-event-plugin": "2.0.1",
    "react-tappable": "1.0.0",
    "react-transition-group": "1.2.0",
    "redux-form": "6.8.0",
    "style-loader": "0.18.2",
    "webpack": "3.0.0",
    "webpack-merge": "4.1.0"
  },
  "devDependencies": {
    "axios": "0.16.2",
    "babel-jest": "20.0.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-register": "6.24.1",
    "enzyme": "2.9.0",
    "esdoc": "0.5.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-loader": "1.8.0",
    "eslint-plugin-css-modules": "2.7.1",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "jest-enzyme": "3.3.0",
    "react-test-renderer": "15.6.1",
    "stylelint": "7.11.1",
    "stylelint-config-css-modules": "1.0.0",
    "stylelint-config-standard": "16.0.0",
    "stylelint-webpack-plugin": "0.7.1",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-server": "2.5.0",
    "webpack-notifier": "1.5.0"
  }
}
