{
  "name": "munchkin-web",
  "version": "3.0.2",
  "main": "./dist/js/app.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config=webpack/config.development.js",
    "dist": "cross-env NODE_ENV=production webpack --config=webpack/config.production.js",
    "doc": "esdoc",
    "i18n:download": "node scripts/i18n/download.js",
    "i18n:translate": "node scripts/i18n/translate.js",
    "i18n:upload": "node scripts/i18n/upload.js",
    "lint:js": "eslint . --ext .js,.jsx",
    "site": "cross-env NODE_ENV=production webpack --config=webpack/config.site.js",
    "start": "cross-env NODE_ENV=production webpack-dev-server --config=webpack/config.site.js",
    "stats": "cross-env NODE_ENV=production webpack --config=webpack/config.stats.js",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "./src/constants/",
      "/node_modules/"
    ],
    "notify": true,
    "transformIgnorePatterns": [
      "node_modules/(?!(munchkin-core)/)"
    ]
  },
  "devDependencies": {
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-version-inline": "1.0.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "classnames": "2.2.5",
    "cname-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "4.1.1",
    "cross-env": "5.0.5",
    "esdoc": "1.0.3",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "1.1.5",
    "history": "4.7.2",
