    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "lodash": "4.17.4",
    "material-ui": "1.0.0-beta.16",
    "material-ui-icons": "1.0.0-beta.15",
    "munchkin-core": "2.1.12",
    "offline-plugin": "4.8.4",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-hot-loader": "3.0.0-beta.7",
    "react-intl": "2.4.0",
    "react-redux": "5.0.6",
    "react-responsive": "2.0.0",
