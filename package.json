    "react-sortable-hoc": "0.6.8",
    "react-swipeable-views": "0.12.8",
    "react-swipeable-views-utils": "0.12.8",
    "react-tappable": "1.0.2",
    "react-test-renderer": "16.0.0",
    "react-transition-group": "2.2.1",
    "redux-form": "7.1.1",
    "redux-thunk": "2.2.0",
    "webpack": "3.7.1",
    "webpack-bundle-analyzer": "2.9.0",
